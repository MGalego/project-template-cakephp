{
  	"name": "qobo/project-template-cakephp",
	"description": "Template for new CakePHP projects",
	"license": "GPL-2.0",
    "require": {
        "alt3/cakephp-swagger": "^1.0",
        "burzum/cakephp-file-storage": "^1.1",
        "burzum/cakephp-imagine-plugin": "^2.1",
        "cakedc/users": "^4.0",
        "cakephp/cakephp": "^3.4",
        "cakephp/debug_kit": "~3.0",
        "cakephp/migrations": "^1.6",
        "cakephp/plugin-installer": "*",
        "dereuromark/cakephp-databaselog": "^2.1",
        "friendsofcake/bootstrap-ui": "~0.3",
        "friendsofcake/crud": "^4.3",
        "fzaninotto/faker": "^1.0",
        "lorenzo/audit-stash": "^1.0",
        "maiconpinto/cakephp-adminlte-theme": "^1.0",
        "mobiledetect/mobiledetectlib": "2.*",
        "pyrech/composer-changelogs": "~1.4",
        "qobo/cakephp-csv-migrations": "^21.0",
        "qobo/cakephp-groups": "^7.0",
        "qobo/cakephp-menu": "^8.0",
        "qobo/cakephp-roles-capabilities": "^11.0",
        "qobo/cakephp-search": "^10.0",
        "qobo/cakephp-utils": "^4.0",
        "qobo/phake-builder": "^3.0"
    },
    "require-dev": {
        "cakephp/bake": "~1.0",
        "cakephp/cakephp-codesniffer": "^3.0",
        "phpunit/phpunit": "*",
        "phpunit/phpunit-selenium": "~3.0",
        "psy/psysh": "@stable",
        "sami/sami": "~3.2"
    },
    "suggest": {
        "ext-xdebug": "Allows code coverage reports and advanced debugging",
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install.",
        "cakephp/cakephp-codesniffer": "Allows to check the code against the coding standards used in CakePHP."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "CsvMigrations\\Test\\": "./vendor/qobo/cakephp-csv-migrations/tests",
            "Groups\\Test\\": "./vendor/qobo/cakephp-groups/tests",
            "RolesCapabilities\\Test\\": "./vendor/qobo/cakephp-roles-capabilities/tests",
            "Menu\\Test\\": "./vendor/qobo/cakephp-menu/tests",
            "Search\\Test\\": "./vendor/qobo/cakephp-search/tests"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
